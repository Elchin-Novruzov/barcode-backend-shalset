{
  "expo": {
    "name": "barcode-scanner",
    "slug": "barcode-scanner",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-shalset.png",
    "scheme": "barcodescanner",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.elxin.barcodescanner"
    },
    "updates": {
      "enabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/logo-shalset.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo-shalset.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "57a89f77-ae09-4a0b-ad18-b7959a0014d2"
      }
    }
  }
}
